<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fintech.intellinews.dao.UserSectionDao">
    <sql id="Base_Column_Select">
        SELECT
          id,
          user_id,
          section_id,
          gmt_create,
          gmt_modified
        FROM oi_user_section
    </sql>

    <insert id="insert" parameterType="UserSectionEntity" useGeneratedKeys="true" keyProperty="id">
        INSERT INTO oi_user_section (id,user_id,section_id,gmt_create)
        VALUES (#{id},#{userId},#{sectionId},#{gmtCreate})
    </insert>
</mapper>